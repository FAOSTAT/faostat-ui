<table class="fs-table" data-role="table"
       {{#if tableOptions}}
           data-striped="true"
           data-search="{{#if tableOptions.data-search}}{{tableOptions.data-search}}{{else}}false{{/if}}"
           data-show-columns="false"
           data-show-header="{{#if tableOptions.data-show-header}}{{tableOptions.data-show-header}}{{else}}true{{/if}}"
           data-pagination="{{#if tableOptions.data-pagination}}{{tableOptions.data-pagination}}{{else}}false{{/if}}"
           data-page-size="{{#if tableOptions.data-page-size}}{{tableOptions.data-page-size}}{{else}}50{{/if}}"
           data-pagination-first-text="{{#if tableOptions.first_text}}{{tableOptions.first_text}}{{else}}First{{/if}}"
           data-pagination-pre-text="{{#if tableOptions.previous_text}}{{tableOptions.previous_text}}{{else}}Previous{{/if}}"
           data-pagination-next-text="{{#if tableOptions.next_text}}{{tableOptions.next_text}}{{else}}Next{{/if}}"
           data-pagination-last-text="{{#if tableOptions.last_text}}{{tableOptions.last_text}}{{else}}Last{{/if}}"

           data-ajax="{{#if tableOptions.data-ajax}}{{tableOptions.data-ajax}}{{/if}}"
           data-side-pagination={{#if tableOptions.data-side-pagination}}{{tableOptions.data-side-pagination}}{{/if}}
       {{/if}}
        {{#if height}}
            data-height="{{height}}"
        {{/if}}
    >
    <thead>
    <tr>
        {{#if sortable}}
            {{#each dsd}}
                <th data-field="{{{this.key}}}" data-sortable="true">{{{this.label}}}</th>
            {{/each}}
        {{else}}
            {{#each dsd}}
                <th data-field="{{{this.key}}}">{{{this.label}}}</th>
            {{/each}}
        {{/if}}
    </tr>
    </thead>
</table>