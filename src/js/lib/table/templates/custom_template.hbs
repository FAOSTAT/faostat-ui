<div class="panel panel-default {{#if class}}{{class}}{{/if}}" {{#if style}}style="{{style}}"{{/if}}>
    <div class="panel-heading">
        {{#if addExport}}
            <div class="pull-right">
                <a class="btn btn-primary" data-role="export">{{export}}</a>
            </div>
        {{/if}}
        <h5>{{title}}</h5>
        <h6>{{subtitle}}</h6>
    </div>
    <div class="panel-body">
        <div data-role="content" {{#if height}}style="height:{{height}}"{{/if}}></div>
    </div>
    {{#if footer}}
        <div class="panel-footer clearfix">{{{footer}}}</div>
    {{/if}}
</div>