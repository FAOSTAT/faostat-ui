<div>
    <h1>{{title}}</h1>
</div>
<!-- Every template has to have a HTML element with data-role="content". It is the place where the chart will be rendered -->

<!-- Add filter -->
<button data-role="add_filter" type="button" class="btn btn-default btn-lg pull-right">
    <span class="glyphicon glyphicon-filter" aria-hidden="true"></span> {{add_filter}}
</button>

<!-- Box Selectors Filters -->
<h2>{{selectors}}</h2>
<div data-role="filters_container">
    <!-- this contains a list of selector-view -->
</div>
<button data-role="apply" type="button" class="btn btn-default btn-lg pull-right">
    <span class="glyphicon glyphicon-triangle-right" aria-hidden="true"></span> {{compare_data}}
</button>

<!-- Timerange -->
<div data-role="timerange">timerange</div>

<!-- Output -->
<div data-role="output">
    <div data-role="chart">chart</div>
    <div data-role="table">table</div>
</div>

