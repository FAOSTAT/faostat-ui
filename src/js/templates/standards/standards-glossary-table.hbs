<table class='table table-striped table-bordered table-responsive table-hover table-condensed'>
    <thead>
    <tr>
        <th>
            {{code_title}}
        </th>
        <th>
            {{label_title}}
        </th>
        <th>
            {{source_title}}
        </th>
    </tr>
    </thead>
    <tbody class="list">
    {{#each rows}}
        <tr>
            {{#if this.code}}
                <td class="fs-mes-code">
                    {{{this.code}}}
                </td>
            {{/if}}
            {{#if this.label}}
                <td class="fs-mes-label">
                    {{{this.label}}}
                </td>
            {{/if}}
            {{#if this.source}}
                <td class="fs-mes-source">
                    {{{this.source}}}
                </td>
            {{/if}}
        </tr>
    {{/each}}
    </tbody>
</table>