<div id='table' class="table-responsive" style="max-height:500px;">
    <table class='table table-striped table-bordered table-hover table-condensed'>
        <thead>
        <tr>
            <th>
                {{code_title}}
            </th>
            <th>
                {{label_title}}
            </th>
            <th>
                {{source_title}}
            </th>
        </tr>
        </thead>
        <tbody class="list">
        {{#each rows}}
            <tr>
                <td class="fs-mes-code">
                    {{{this.code}}}
                </td>
                <td class="fs-mes-label">
                    {{{this.label}}}
                </td>
                <td class="fs-mes-source">
                    {{{this.source}}}
                </td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>