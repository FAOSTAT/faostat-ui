<div class='row'>

    <div class='col-md-9 fs-data-padding-top'>

        <!-- Container tab-panels -->
        <div class="tab-content">
            <div class="tab-pane" id="about" data-section='about'></div>
            <div class="tab-pane" id="interactive_download" data-section='interactive_download'></div>
            <div class="tab-pane" id="report" data-section='report'></div>
            <div class="tab-pane" id="bulk_downloads" data-section='bulk_downloads'></div>
            <div class="tab-pane" id="metadata" data-section='metadata'></div>
            <div class="tab-pane" id="browse" data-section='browse_by_domain_code'></div>
        </div>

    </div>

    <div class='col-md-3 fs-data-padding-top'>

        <div class="panel panel-default hoverable">
            <div class="panel-heading">
                <h4 data-role="domain-title" class="fs-download-domain-title"></h4>
            </div>

            <div class="panel-body grey lighten-5" >

                <!-- Description -->
                <div data-role="description">
                    <p data-role="text"></p>
                </div>

                <!-- Organization -->
                <div data-role="organization">
                    <small data-role="text"></small>
                </div>

            </div>

            <div class="panel-footer amber lighten-4" data-role="bulk-downloads-panel">

                <div>
                    <h5>
                        <i class="material-icons left">file_download</i>{{bulk_downloads}}
                    </h5>
                    <ul class="list-unstyled list-download" data-role="bulk-downloads-caret"></ul>
                </div>
            </div>

            <div class="panel-footer grey lighten-5">

                <!-- Last Update -->
                <div data-role="last-update">
                    <p class="label label-default label-aligned">{{last_update}}</p>
                    <p data-role="date"></p>
                </div>

                <!-- Contacts -->
                <!--                <div data-role="contact-name">
                    <p class="label label-default label-aligned">{{contacts}}Contacts</p>

                    <p data-role="text"></p>
                </div>-->

                <!--&lt;!&ndash; Group &ndash;&gt;-->
                <!--<div data-role="">-->
                <!--<p class="label label-default label-aligned">Group</p>-->
                <!--<p><span data-role="go_to_download" class="domain-icon icojam_Q"></span></p>-->
                <!--<p data-role="text">Commodity Balances - Livestock and Fish Primary Equivalent</p>-->
                <!--</div>-->

                <!-- Email -->
                <div data-role="contact-email" style="display:none">
                    <p class="label label-default label-aligned">{{contacts_email}}</p>

                    <p data-role="text"></p>
                </div>

                <div data-role="related-documents">
                    <p class="label label-default label-aligned">{{related_documents}}</p>

                    <!--TODO: remove when data are not available -->
                    <div data-role="fs-download-related-documents"></div>
                </div>

                <a class="btn btn-link btn-block waves-effect truncate" target='_blank' data-role="fs-download-definitions-button">
                    <i class="material-icons left">list</i>{{definitions_and_standards}}
                </a>

                <a class="btn btn-link btn-block waves-effect" target='_blank' data-role="fs-download-metadata-button">
                    <i class="material-icons left">info_outline</i>{{full_metadata}}
                </a>

            </div>
        </div>
    </div>

</div>

<br>

<div class='row' id="fs-download-output-container" style="display:none;">

    <div class='col-xs-12'>
        <div class="fs-preview-container">
            <div class="panel panel-default hoverable">

                <div class="panel-heading clearfix">
                    <div class="pull-right">
                        <a class="btn-floating waves-effect waves-light btn-default disabled"
                           data-role="export">
                            <i class="material-icons">file_download</i>
                        </a>
                    </div>
                    <h4>{{show_data}}</h4>
                </div>

                <div class="panel-body fs-output-preview">

                    <!-- message preview -->
                    <div data-role="message">
                        <h3 class="text-center">
                            {{where_is_the_data}}
                        </h3>

                        <h5 class="text-center fs-output-preview-message">
                            <span class="fs-icojam-icon icojam_rotate_4"></span><br>
                            {{{show_data_info_message}}}
                        </h5>
                    </div>

                    <!-- output content -->
                    <div data-role="content" class="fs-download-output-area" ></div>

                </div>
            </div>
        </div>
    </div>

</div>
