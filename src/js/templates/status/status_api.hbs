<script type='text/x-handlebars-template' id='summary_item'>
    {{#each items}}
        <div class='summary-item'
             data-role='summary-item'
             data-id='{{id}}'
             data-code='{{code}}'>
            {{label}}
        </div>
    {{/each}}
</script>
